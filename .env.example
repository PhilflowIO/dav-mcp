# CalDAV/CardDAV Server Configuration
CALDAV_SERVER_URL=https://dav.example.com
CALDAV_USERNAME=your-username
CALDAV_PASSWORD=your-password-here

# MCP Server Configuration
PORT=3000
MCP_SERVER_NAME=tsdav-mcp-server
MCP_SERVER_VERSION=1.0.0

# CORS Configuration (comma-separated list of allowed origins, or * for all)
CORS_ALLOWED_ORIGINS=http://localhost:5678,http://localhost:3000

# Authentication (REQUIRED - for n8n Bearer Auth)
BEARER_TOKEN=your-secure-token-here

# Environment
NODE_ENV=production
